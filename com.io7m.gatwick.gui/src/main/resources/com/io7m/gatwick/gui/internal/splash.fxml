<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>

<StackPane fx:id="splashPane" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #000000;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.io7m.gatwick.gui.internal.splash.GWSplashController">
   <children>
      <ImageView fx:id="splashLogo" fitHeight="256.0" fitWidth="512.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@gatwick.png" />
         </image>
      </ImageView>
      <ProgressBar fx:id="splashProgress" prefHeight="8.0" prefWidth="256.0">
         <StackPane.margin>
            <Insets top="48.0" />
         </StackPane.margin></ProgressBar>
      <Label fx:id="splashText" text="Filler">
         <StackPane.margin>
            <Insets top="96.0" />
         </StackPane.margin>
      </Label>
   </children>
</StackPane>
